{
    "name": "@raw-multicall/ethers-v5-helper",
    "version": "1.0.0",
    "packageManager": "yarn@3.3.1",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "devDependencies": {
        "@raw-multicall/test-helper": "workspace:*",
        "@typechain/ethers-v5": "^11.0.0",
        "@typechain/ethers-v6": "^0.4.0",
        "@typechain/hardhat": "^8.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-unused-imports": "^3.0.0",
        "hardhat": "^2.16.1",
        "jest": "^29.6.1",
        "prettier": "^3.0.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    },
    "scripts": {
        "typecheck": "yarn tsc --noEmit",
        "build": "yarn tsc -p tsconfig.build.json",
        "lint:nofix": "yarn eslint \"{src,test}/**/*.{ts,js}\" --quiet",
        "lint": "yarn lint:nofix --fix",
        "test": "yarn jest",
        "compile-contracts-ethers-v5": "yarn hardhat compile",
        "compile-contracts": "yarn compile-contracts-ethers-v5 --force"
    },
    "dependencies": {
        "@raw-multicall/core": "workspace:*",
        "ethers": "^5.7.0"
